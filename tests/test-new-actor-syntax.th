dolphin(int weight, int age):
    print(weight);
    print(age)
    receive:
        eat(int food):
            weight = weight + food
        growOld(int time):
            age = age + time
        eatAndGrowOld(int food, int time):
            weight = weight + food
            age = age + time
    drop:
        print(weight)
        print(age)
    after:
        return;
        
func main() -> int:
    int weight;
    int age;
    weight = 10;
    age = 3;
    actor d
    d = new dolphin(weight, age)    
    print("new dolphin created")
    return 0
