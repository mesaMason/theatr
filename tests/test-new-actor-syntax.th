dolphin(int weight, int age):
    receive:
        eat(int food):
            weight = weight + food
    drop:
        return;
    after:
        return;
        
func main() -> int:
    int weight;
    int age;
    weight = 10;
    age = 3;
    actor d
    d = new dolphin(weight, age)    
    print("new dolphin created")
    return 0